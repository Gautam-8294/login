    <title>Login</title>
    <link rel="stylesheet" href="css/styles.css">
</head>
<body>
    <form action="/login" method="post">
        <div class="form">
            <div class="title">Welcome</div>
            <div class="subtitle">Log in your account!</div>
            <div class="input-container ic2">
              <input name="loginUserEmail" id="lastname" class="input" type="email" placeholder=" "  required/>
              <div class="cut"></div>
              <label for="email" class="placeholder">Email</label>
            </div>
            <div class="input-container ic2">
              <input name="loginPassword" id="email" class="input" type="password" placeholder=" " required/>
              <div class="cut cut-short"></div>
              <label for="password" class="placeholder">Password</>
            </div>
            <button name= type="submit" class="submit">Log in</button>
          </div>
    </form>
</body>
</html>